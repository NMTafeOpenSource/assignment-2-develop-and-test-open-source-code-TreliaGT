<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="476.0" prefWidth="661.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="startervehicle.FXMLDocumentController">
    <children>
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
        <Label layoutX="14.0" layoutY="14.0" prefHeight="32.0" prefWidth="114.0" text="CarRental">
            <font>
                <Font size="25.0" />
            </font>
        </Label>
        <TableView layoutX="7.0" layoutY="73.0" prefHeight="321.0" prefWidth="644.0">
            <columns>
                <TableColumn fx:id="VehicleColumn" prefWidth="150.0" text="Vehicle">  
                    <cellValueFactory>
                        <PropertyValueFactory property="Vehicle" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn  prefWidth="143.0" text="Registration">  
                    <cellValueFactory>
                        <PropertyValueFactory property="Registration" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn  prefWidth="138.0" text="Total km travelled"> 
                    <cellValueFactory>
                        <PropertyValueFactory property="Totalkilometrestravelled" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn  prefWidth="96.0" text="Total Services"> 
                    <cellValueFactory>
                        <PropertyValueFactory property="Totalservices" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn  prefWidth="114.0" text="Requires a Service"> 
                    <cellValueFactory>
                        <PropertyValueFactory property="requiresService" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
        <Button layoutX="7.0" layoutY="425.0" mnemonicParsing="false" onAction="#AddButton" prefHeight="36.0" prefWidth="77.0" text="Add" />
        <Button layoutX="99.0" layoutY="425.0" mnemonicParsing="false" onAction="#Delete" prefHeight="36.0" prefWidth="77.0" text="Delete" />
        <Button layoutX="498.0" layoutY="425.0" mnemonicParsing="false" onAction="#ViewDetail" prefHeight="36.0" prefWidth="153.0" text="View Vehicle Details" />
    </children>
</AnchorPane>
